{
    "name": "zanysoft/laravel-roles",
    "description": "Powerful package for handling roles and permissions in Laravel. Supports Laravel 5.3+",
    "keywords": [
        "roles",
        "permissions",
        "acl",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mubashar Ahmad",
            "email": "zanysoft@outlook.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*|5.4.*|5.5.*|5.6.*|5.7.*"
    },
    "autoload": {
        "files": [
            "src/Helpers/helpers.php"
        ],
        "psr-4": {
            "ZanySoft\\LaravelRoles\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZanySoft\\LaravelRoles\\RolesServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
